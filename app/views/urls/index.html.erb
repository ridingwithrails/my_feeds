<h1>Listing urls</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @urls.each do |site| %>
  <tr>
	<td><%= link_to site.url %><td>
	<td><%= link_to 'Show', site %></td>
    <td><%= link_to 'Edit', edit_url_path(site) %></td>
    <td><%= link_to 'Destroy', site, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

